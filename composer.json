{
    "name": "internetrix/silverstripe-events",
    "description": "A module for adding an events system",
    "type": "silverstripe-vendormodule",
    "repositories": [
        {
            "type": "git",
            "url": "git@gitlab.internetrix.net:silverstripe-modules/listingsummary.git",
            "name": "internetrix/silverstripe-listingsummary"
        }
    ],
    "keywords": [
        "silverstripe",
        "events",
        "calendar",
        "event"
    ],
    "authors": [
        {
            "name": "Stewart Wilson",
            "email": "stewart.wilson@internetrix.com.au"
        }
    ],
    "require": {
        "silverstripe/framework": "^4.4",
        "silverstripe/cms": "^4.4",
        "silverstripe/admin": "^1.3",
        "internetrix/silverstripe-listingsummary": "dev-master",
        "silverstripe/lumberjack": "^2.0",
        "tractorcow/silverstripe-colorpicker": "^4.0",
        "symbiote/silverstripe-addressable": "^5.0",
        "symbiote/silverstripe-gridfieldextensions": "^3.2",
        "symbiote/silverstripe-grouped-cms-menu": "^4.0",
        "silverstripe/userforms": "^5.4"
    },
    "extra": {
        "installer-name": "events",
        "expose": [
            "css",
            "images",
            "javascript",
            "thirdparty"
        ]
    },
    "replace": {
        "silverstripe-modules/events": "self.version"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
